  {
   "cell_type": "code",
   "execution_count": 19,
   "metadata": {},
   "outputs": [],
   "source": [
    "using Plots\n",
    "\n",
    "μ = 0\n",
    "ϵ = -5+μ:0.1:5+μ\n",
    "T_list = [500,1000]\n",
    "\n",
    "for T ∈ T_list\n",
    "    fd = 1 ./ (ℯ.^((ϵ.-μ)./T).+1)\n",
    "    plot!(ϵ, fd, label=\"T=$T\")\n",
    "end\n",
    "savefig(\"fermi-dirac.png\")"
   ]
  },
